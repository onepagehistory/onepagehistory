{
  "name": "worldhistorypage",
  "private": true,
  "scripts": {
    "start": "react-static start",
    "build-staging": "react-static build --staging",
    "build": "react-static build",
    "serve": "serve dist -p 8080 -s",
    "prepare-staging-publish": "echo \"stg1.worldhistory.page\" > ./dist/CNAME && echo \"\" > ./dist/GA.js",
    "publish": "gh-pages -d dist -t -r https://github.com/onepagehistory/onepagehistory.github.io.git -b master -m \"Updates $(git log -1 --pretty=format:'%h - %s' --abbrev-commit)\"",
    "staging-publish": "npm run prepare-staging-publish && gh-pages -d dist -t -r https://github.com/onepagehistory/staging-1.git -b master -m \"Updates $(git log -1 --pretty=format:'%h - %s' --abbrev-commit)\""
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-markdown": "4.3.1",
    "react-router-dom": "5.2.0",
    "react-static": "7.4.2",
    "react-static-plugin-react-router": "7.4.2",
    "react-static-plugin-sass": "7.3.0",
    "react-static-plugin-sitemap": "7.4.2",
    "react-static-plugin-source-filesystem": "7.4.2",
    "react-static-plugin-typescript": "7.4.2"
  },
  "devDependencies": {
    "@types/node": "14.0.22",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@types/react-hot-loader": "4.1.1",
    "@types/react-router-dom": "5.1.5",
    "@types/webpack-env": "1.15.2",
    "chokidar": "3.4.0",
    "gh-pages": "3.1.0",
    "jimp": "0.14.0",
    "node-sass-chokidar": "1.5.0",
    "npm-run-all": "4.1.5",
    "serve": "11.3.2",
    "typescript": "3.9.6"
  }
}
