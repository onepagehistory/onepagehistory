{
  "name": "worldhistorypage",
  "private": true,
  "scripts": {
    "start": "react-static start",
    "build-staging": "react-static build --staging",
    "build": "react-static build",
    "serve": "serve dist -p 8080 -s",
    "prepare-staging-publish": "echo \"stg1.worldhistory.page\" > ./dist/CNAME && echo \"\" > ./dist/GA.js",
    "publish": "gh-pages -d dist -t -r https://github.com/onepagehistory/onepagehistory.github.io.git -b master -m \"Updates $(git log -1 --pretty=format:'%h - %s' --abbrev-commit)\"",
    "staging-publish": "npm run prepare-staging-publish && gh-pages -d dist -t -r https://github.com/onepagehistory/staging-1.git -b master -m \"Updates $(git log -1 --pretty=format:'%h - %s' --abbrev-commit)\""
  },
  "dependencies": {
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-markdown": "4.2.2",
    "react-router-dom": "5.0.1",
    "react-static": "7.2.2",
    "react-static-plugin-react-router": "7.2.2",
    "react-static-plugin-sass": "7.2.2",
    "react-static-plugin-sitemap": "7.2.2",
    "react-static-plugin-source-filesystem": "7.2.2",
    "react-static-plugin-typescript": "7.2.2"
  },
  "devDependencies": {
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-router-dom": "4.3.5",
    "@types/webpack-env": "1.14.0",
    "chokidar": "3.1.1",
    "gh-pages": "2.1.1",
    "jimp": "0.8.4",
    "node-sass-chokidar": "1.3.5",
    "npm-run-all": "4.1.5",
    "serve": "11.1.0",
    "typescript": "3.6.3"
  }
}
